{
  "version": "1.0.0",
  "templates": [
    {
      "hookName": "useButton",
      "htmlElement": "button",
      "jsxPattern": "<button {...buttonProps}>{children}</button>",
      "accessibility": {
        "role": "button",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label for screen readers when button text is not descriptive"
          },
          {
            "name": "aria-pressed",
            "required": false,
            "validValues": ["true", "false", "mixed"],
            "description": "Indicates the current pressed state for toggle buttons"
          },
          {
            "name": "aria-disabled",
            "required": false,
            "validValues": ["true", "false"],
            "description": "Indicates whether the button is disabled"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Enter",
            "action": "Activates the button",
            "required": true
          },
          {
            "key": "Space",
            "action": "Activates the button",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "examples": [
        "<button {...buttonProps}>Click me</button>",
        "<button {...buttonProps} aria-label=\"Submit form\">Submit</button>"
      ]
    },
    {
      "hookName": "useToggleButton",
      "htmlElement": "button",
      "jsxPattern": "<button {...buttonProps} aria-pressed={isSelected}>{children}</button>",
      "accessibility": {
        "role": "button",
        "ariaAttributes": [
          {
            "name": "aria-pressed",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates the toggle state"
          },
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label when visual label is not sufficient"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Enter",
            "action": "Toggles the button state",
            "required": true
          },
          {
            "key": "Space",
            "action": "Toggles the button state",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "examples": [
        "<button {...buttonProps} aria-pressed={isSelected}>Toggle me</button>"
      ]
    },
    {
      "hookName": "useSwitch",
      "htmlElement": "button",
      "jsxPattern": "<button {...inputProps} role=\"switch\" aria-checked={isSelected}>{children}</button>",
      "accessibility": {
        "role": "switch",
        "ariaAttributes": [
          {
            "name": "aria-checked",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether the switch is on or off"
          },
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label for the switch"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Enter",
            "action": "Toggles the switch",
            "required": true
          },
          {
            "key": "Space",
            "action": "Toggles the switch",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "examples": [
        "<button {...inputProps} role=\"switch\" aria-checked={isSelected}>Enable notifications</button>"
      ]
    },
    {
      "hookName": "useCheckbox",
      "htmlElement": "input",
      "jsxPattern": "<input {...inputProps} type=\"checkbox\" />",
      "accessibility": {
        "role": "checkbox",
        "ariaAttributes": [
          {
            "name": "aria-checked",
            "required": true,
            "validValues": ["true", "false", "mixed"],
            "description": "Indicates the checked state (mixed for indeterminate)"
          },
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label when no visible label exists"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Space",
            "action": "Toggles the checkbox",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "examples": [
        "<input {...inputProps} type=\"checkbox\" />",
        "<label><input {...inputProps} type=\"checkbox\" /> Accept terms</label>"
      ]
    },
    {
      "hookName": "useRadio",
      "htmlElement": "input",
      "jsxPattern": "<input {...inputProps} type=\"radio\" />",
      "accessibility": {
        "role": "radio",
        "ariaAttributes": [
          {
            "name": "aria-checked",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether the radio is selected"
          },
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label when no visible label exists"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "ArrowDown",
            "action": "Selects next radio in group",
            "required": true
          },
          {
            "key": "ArrowUp",
            "action": "Selects previous radio in group",
            "required": true
          },
          {
            "key": "Space",
            "action": "Selects the radio",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "examples": [
        "<label><input {...inputProps} type=\"radio\" name=\"option\" /> Option 1</label>"
      ]
    },
    {
      "hookName": "useTextField",
      "htmlElement": "input",
      "jsxPattern": "<div>\n  <label {...labelProps}>{label}</label>\n  <input {...inputProps} />\n  {description && <div {...descriptionProps}>{description}</div>}\n  {isInvalid && <div {...errorMessageProps}>{errorMessage}</div>}\n</div>",
      "accessibility": {
        "role": "textbox",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label when no visible label exists"
          },
          {
            "name": "aria-describedby",
            "required": false,
            "description": "References description and error message elements"
          },
          {
            "name": "aria-invalid",
            "required": false,
            "validValues": ["true", "false"],
            "description": "Indicates validation state"
          },
          {
            "name": "aria-required",
            "required": false,
            "validValues": ["true", "false"],
            "description": "Indicates whether the field is required"
          }
        ],
        "keyboardNavigation": [],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "label",
            "propsObject": "labelProps",
            "order": 1,
            "purpose": "Provides accessible label for input"
          },
          {
            "element": "input",
            "propsObject": "inputProps",
            "order": 2,
            "purpose": "Main text input field"
          },
          {
            "element": "div",
            "propsObject": "descriptionProps",
            "order": 3,
            "purpose": "Provides help text or description"
          },
          {
            "element": "div",
            "propsObject": "errorMessageProps",
            "order": 4,
            "purpose": "Displays validation error message"
          }
        ],
        "compositionPattern": "Label + Input + Optional Description + Optional Error Message",
        "requiresWrapper": true
      },
      "examples": [
        "<div>\n  <label {...labelProps}>Email</label>\n  <input {...inputProps} />\n  <div {...descriptionProps}>We'll never share your email</div>\n</div>"
      ]
    },
    {
      "hookName": "useDialog",
      "htmlElement": "dialog",
      "jsxPattern": "<dialog {...dialogProps}>\n  <h2 {...titleProps}>{title}</h2>\n  <div>{children}</div>\n  <button onClick={close}>Close</button>\n</dialog>",
      "accessibility": {
        "role": "dialog",
        "ariaAttributes": [
          {
            "name": "aria-labelledby",
            "required": true,
            "description": "References the dialog title element"
          },
          {
            "name": "aria-describedby",
            "required": false,
            "description": "References the dialog description element"
          },
          {
            "name": "aria-modal",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether the dialog is modal"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Escape",
            "action": "Closes the dialog",
            "required": true
          },
          {
            "key": "Tab",
            "action": "Cycles focus through dialog elements",
            "required": true
          }
        ],
        "focusManagement": "Trap focus within dialog when open, restore focus to trigger when closed",
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "dialog",
            "propsObject": "dialogProps",
            "order": 1,
            "purpose": "Main dialog container"
          },
          {
            "element": "h2",
            "propsObject": "titleProps",
            "order": 2,
            "purpose": "Dialog title for aria-labelledby"
          },
          {
            "element": "div",
            "order": 3,
            "purpose": "Dialog content container"
          },
          {
            "element": "button",
            "order": 4,
            "purpose": "Close button for dialog"
          }
        ],
        "compositionPattern": "Dialog wrapper + Title + Content + Close button",
        "requiresWrapper": false
      },
      "examples": [
        "<dialog {...dialogProps} aria-labelledby=\"dialog-title\" aria-modal=\"true\">\n  <h2 {...titleProps} id=\"dialog-title\">Confirm Action</h2>\n  <div>Are you sure?</div>\n  <button onClick={close}>Close</button>\n</dialog>"
      ],
      "integrationNotes": [
        "Often contains useButton for actions",
        "May use useTextField for input dialogs"
      ]
    },
    {
      "hookName": "useModal",
      "htmlElement": "div",
      "jsxPattern": "<div {...modalProps} role=\"dialog\" aria-modal=\"true\">\n  <h2 {...titleProps}>{title}</h2>\n  <div>{children}</div>\n</div>",
      "accessibility": {
        "role": "dialog",
        "ariaAttributes": [
          {
            "name": "aria-labelledby",
            "required": true,
            "description": "References the modal title element"
          },
          {
            "name": "aria-modal",
            "required": true,
            "validValues": ["true"],
            "description": "Must be true for modals"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Escape",
            "action": "Closes the modal",
            "required": true
          },
          {
            "key": "Tab",
            "action": "Cycles focus within modal",
            "required": true
          }
        ],
        "focusManagement": "Trap focus within modal, prevent interaction with background content",
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "div",
            "propsObject": "modalProps",
            "order": 1,
            "purpose": "Modal container with backdrop"
          },
          {
            "element": "h2",
            "propsObject": "titleProps",
            "order": 2,
            "purpose": "Modal title"
          },
          {
            "element": "div",
            "order": 3,
            "purpose": "Modal content"
          }
        ],
        "compositionPattern": "Modal container + Title + Content",
        "requiresWrapper": true
      }
    },
    {
      "hookName": "usePopover",
      "htmlElement": "div",
      "jsxPattern": "<div>\n  <button {...triggerProps}>{triggerLabel}</button>\n  {isOpen && <div {...popoverProps}>{children}</div>}\n</div>",
      "accessibility": {
        "ariaAttributes": [
          {
            "name": "aria-expanded",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether popover is expanded"
          },
          {
            "name": "aria-haspopup",
            "required": true,
            "validValues": ["true", "menu", "dialog"],
            "description": "Indicates the trigger opens a popover"
          },
          {
            "name": "aria-controls",
            "required": true,
            "description": "References the popover element ID"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Escape",
            "action": "Closes the popover",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "button",
            "propsObject": "triggerProps",
            "order": 1,
            "purpose": "Trigger button for popover"
          },
          {
            "element": "div",
            "propsObject": "popoverProps",
            "order": 2,
            "purpose": "Popover content container"
          }
        ],
        "compositionPattern": "Trigger + Conditionally rendered popover",
        "requiresWrapper": true
      }
    },
    {
      "hookName": "useTooltip",
      "htmlElement": "div",
      "jsxPattern": "<div>\n  <div {...triggerProps}>{children}</div>\n  {isOpen && <div {...tooltipProps} role=\"tooltip\">{tooltipContent}</div>}\n</div>",
      "accessibility": {
        "role": "tooltip",
        "ariaAttributes": [
          {
            "name": "aria-describedby",
            "required": true,
            "description": "Trigger references tooltip element ID"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Escape",
            "action": "Closes the tooltip",
            "required": true
          }
        ],
        "wcagLevel": "AA",
        "notes": [
          "Tooltip should not contain interactive elements",
          "Use popover for interactive content"
        ]
      },
      "nestedStructure": {
        "components": [
          {
            "element": "div",
            "propsObject": "triggerProps",
            "order": 1,
            "purpose": "Trigger element that shows tooltip on hover/focus"
          },
          {
            "element": "div",
            "propsObject": "tooltipProps",
            "order": 2,
            "purpose": "Tooltip content"
          }
        ],
        "compositionPattern": "Trigger + Conditionally rendered tooltip",
        "requiresWrapper": true
      }
    },
    {
      "hookName": "useTabs",
      "htmlElement": "div",
      "jsxPattern": "<div>\n  <div {...tabListProps} role=\"tablist\">\n    {tabs.map((tab) => <button {...tabProps(tab.key)} role=\"tab\">{tab.label}</button>)}\n  </div>\n  <div {...tabPanelProps} role=\"tabpanel\">{selectedPanel}</div>\n</div>",
      "accessibility": {
        "role": "tablist",
        "ariaAttributes": [
          {
            "name": "aria-selected",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates selected tab"
          },
          {
            "name": "aria-controls",
            "required": true,
            "description": "Tab references its panel ID"
          },
          {
            "name": "aria-labelledby",
            "required": true,
            "description": "Panel references its tab ID"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "ArrowLeft",
            "action": "Moves focus to previous tab",
            "required": true
          },
          {
            "key": "ArrowRight",
            "action": "Moves focus to next tab",
            "required": true
          },
          {
            "key": "Home",
            "action": "Moves focus to first tab",
            "required": true
          },
          {
            "key": "End",
            "action": "Moves focus to last tab",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "div",
            "propsObject": "tabListProps",
            "order": 1,
            "purpose": "Container for tab buttons"
          },
          {
            "element": "button",
            "propsObject": "tabProps",
            "order": 2,
            "purpose": "Individual tab button"
          },
          {
            "element": "div",
            "propsObject": "tabPanelProps",
            "order": 3,
            "purpose": "Content panel for selected tab"
          }
        ],
        "compositionPattern": "Tab list + Multiple tab buttons + Tab panels",
        "requiresWrapper": true
      }
    },
    {
      "hookName": "useBreadcrumbs",
      "htmlElement": "nav",
      "jsxPattern": "<nav {...navProps} aria-label=\"Breadcrumb\">\n  <ol {...listProps}>\n    {items.map((item, index) => (\n      <li {...itemProps} key={index}>\n        <a href={item.href}>{item.label}</a>\n      </li>\n    ))}\n  </ol>\n</nav>",
      "accessibility": {
        "role": "navigation",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": true,
            "validValues": ["Breadcrumb"],
            "description": "Identifies the navigation as breadcrumbs"
          },
          {
            "name": "aria-current",
            "required": true,
            "validValues": ["page"],
            "description": "Marks the current page in breadcrumb trail"
          }
        ],
        "keyboardNavigation": [],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "nav",
            "propsObject": "navProps",
            "order": 1,
            "purpose": "Navigation landmark"
          },
          {
            "element": "ol",
            "propsObject": "listProps",
            "order": 2,
            "purpose": "Ordered list of breadcrumb items"
          },
          {
            "element": "li",
            "propsObject": "itemProps",
            "order": 3,
            "purpose": "Individual breadcrumb item"
          }
        ],
        "compositionPattern": "Nav + Ordered list + List items with links",
        "requiresWrapper": false
      }
    },
    {
      "hookName": "useMenu",
      "htmlElement": "div",
      "jsxPattern": "<div {...menuProps} role=\"menu\">\n  {items.map((item) => (\n    <div {...menuItemProps(item.key)} role=\"menuitem\">{item.label}</div>\n  ))}\n</div>",
      "accessibility": {
        "role": "menu",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label for the menu"
          },
          {
            "name": "aria-orientation",
            "required": false,
            "validValues": ["vertical", "horizontal"],
            "description": "Indicates menu orientation"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "ArrowDown",
            "action": "Moves focus to next menu item",
            "required": true
          },
          {
            "key": "ArrowUp",
            "action": "Moves focus to previous menu item",
            "required": true
          },
          {
            "key": "Home",
            "action": "Moves focus to first menu item",
            "required": true
          },
          {
            "key": "End",
            "action": "Moves focus to last menu item",
            "required": true
          },
          {
            "key": "Escape",
            "action": "Closes the menu",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "div",
            "propsObject": "menuProps",
            "order": 1,
            "purpose": "Menu container"
          },
          {
            "element": "div",
            "propsObject": "menuItemProps",
            "order": 2,
            "purpose": "Individual menu items"
          }
        ],
        "compositionPattern": "Menu container + Multiple menu items",
        "requiresWrapper": false
      }
    },
    {
      "hookName": "useDropdown",
      "htmlElement": "div",
      "jsxPattern": "<div>\n  <button {...triggerProps}>{triggerLabel}</button>\n  {isOpen && <div {...menuProps} role=\"menu\">{children}</div>}\n</div>",
      "accessibility": {
        "ariaAttributes": [
          {
            "name": "aria-haspopup",
            "required": true,
            "validValues": ["menu"],
            "description": "Indicates the trigger opens a menu"
          },
          {
            "name": "aria-expanded",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether dropdown is expanded"
          },
          {
            "name": "aria-controls",
            "required": true,
            "description": "References the menu element ID"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Enter",
            "action": "Opens/closes the dropdown",
            "required": true
          },
          {
            "key": "Escape",
            "action": "Closes the dropdown",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "button",
            "propsObject": "triggerProps",
            "order": 1,
            "purpose": "Trigger button for dropdown"
          },
          {
            "element": "div",
            "propsObject": "menuProps",
            "order": 2,
            "purpose": "Dropdown menu content"
          }
        ],
        "compositionPattern": "Trigger + Conditionally rendered menu",
        "requiresWrapper": true
      },
      "integrationNotes": [
        "Often uses useMenu for menu implementation"
      ]
    },
    {
      "hookName": "useAccordion",
      "htmlElement": "div",
      "jsxPattern": "<div {...accordionProps}>\n  {items.map((item) => (\n    <div {...itemProps(item.key)} key={item.key}>\n      <button>{item.title}</button>\n      {isExpanded(item.key) && <div>{item.content}</div>}\n    </div>\n  ))}\n</div>",
      "accessibility": {
        "ariaAttributes": [
          {
            "name": "aria-expanded",
            "required": true,
            "validValues": ["true", "false"],
            "description": "Indicates whether accordion item is expanded"
          },
          {
            "name": "aria-controls",
            "required": true,
            "description": "Button references content panel ID"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "Enter",
            "action": "Toggles accordion item",
            "required": true
          },
          {
            "key": "Space",
            "action": "Toggles accordion item",
            "required": true
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "div",
            "propsObject": "accordionProps",
            "order": 1,
            "purpose": "Accordion container"
          },
          {
            "element": "div",
            "propsObject": "itemProps",
            "order": 2,
            "purpose": "Individual accordion items"
          },
          {
            "element": "button",
            "order": 3,
            "purpose": "Accordion item trigger"
          },
          {
            "element": "div",
            "order": 4,
            "purpose": "Accordion item content panel"
          }
        ],
        "compositionPattern": "Accordion container + Items (Button + Content panel)",
        "requiresWrapper": false
      }
    },
    {
      "hookName": "useTable",
      "htmlElement": "table",
      "jsxPattern": "<table {...tableProps}>\n  <thead>\n    <tr>{headers.map((h) => <th>{h}</th>)}</tr>\n  </thead>\n  <tbody>\n    {rows.map((row) => (\n      <tr {...rowProps(row.key)}>\n        {row.cells.map((cell) => <td {...cellProps}>{cell}</td>)}\n      </tr>\n    ))}\n  </tbody>\n</table>",
      "accessibility": {
        "role": "table",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": false,
            "description": "Accessible label for the table"
          },
          {
            "name": "aria-rowcount",
            "required": false,
            "description": "Total number of rows (for virtualized tables)"
          },
          {
            "name": "aria-selected",
            "required": false,
            "validValues": ["true", "false"],
            "description": "Indicates selected rows"
          }
        ],
        "keyboardNavigation": [
          {
            "key": "ArrowDown",
            "action": "Moves focus to next row",
            "required": false
          },
          {
            "key": "ArrowUp",
            "action": "Moves focus to previous row",
            "required": false
          }
        ],
        "wcagLevel": "AA"
      },
      "nestedStructure": {
        "components": [
          {
            "element": "table",
            "propsObject": "tableProps",
            "order": 1,
            "purpose": "Table container"
          },
          {
            "element": "tr",
            "propsObject": "rowProps",
            "order": 2,
            "purpose": "Table rows"
          },
          {
            "element": "td",
            "propsObject": "cellProps",
            "order": 3,
            "purpose": "Table cells"
          }
        ],
        "compositionPattern": "Table + Thead/Tbody + Rows + Cells",
        "requiresWrapper": false
      }
    },
    {
      "hookName": "usePagination",
      "htmlElement": "nav",
      "jsxPattern": "<nav {...paginationProps} aria-label=\"Pagination\">\n  <button disabled={currentPage === 1}>Previous</button>\n  {pageNumbers.map((page) => <button>{page}</button>)}\n  <button disabled={currentPage === totalPages}>Next</button>\n</nav>",
      "accessibility": {
        "role": "navigation",
        "ariaAttributes": [
          {
            "name": "aria-label",
            "required": true,
            "validValues": ["Pagination"],
            "description": "Identifies the navigation as pagination"
          },
          {
            "name": "aria-current",
            "required": true,
            "validValues": ["page"],
            "description": "Marks the current page button"
