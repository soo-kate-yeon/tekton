{
  "name": "@tekton/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Tekton design token generator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tekton": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src tests --ext .ts"
  },
  "dependencies": {
    "@tekton/contracts": "workspace:*",
    "@tekton/theme": "workspace:*",
    "@tekton/token-generator": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "enquirer": "^2.4.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
